<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Debug</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>localhost:2797/AHMWebAPI</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>localhost:2797</MSDeployServiceURL>
    <DeployIisAppPath>AHMWebAPI</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>InProc</MSDeployPublishMethod>
    <EnableMSDeployBackup>False</EnableMSDeployBackup>
    <UserName />
    <_SavePWD>False</_SavePWD>
    <PublishDatabaseSettings>
      <Objects>
        <ObjectGroup Name="AhmDbContext" Order="1" Enabled="False">
          <Destination Path="Data Source=(local);Initial Catalog=ApartmentHouseManagement;Integrated Security=True" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="AHM.DataLayer.AhmContext, AHM.DataLayer" Origin="Convention" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="AhmContext" Order="2" Enabled="False">
          <Destination Path="Data Source=(local);Initial Catalog=ApartmentHouseManagement;Integrated Security=True" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=(local);Initial Catalog=ApartmentHouseManagement;Integrated Security=True" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\AhmContext_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=(local);Initial Catalog=ApartmentHouseManagement;Integrated Security=SSPI;" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)AhmContext-Web.config Connection String">
      <ParameterValue>Data Source=(local);Initial Catalog=ApartmentHouseManagement;Integrated Security=True</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)AhmDbContext-Web.config Connection String">
      <ParameterValue>Data Source=(local);Initial Catalog=ApartmentHouseManagement;Integrated Security=True</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
  <ItemGroup>
    <_ConnectionStringsToInsert Include="AhmDbContext" />
  </ItemGroup>
</Project>